arm-none-eabi-gcc -c -g -Wall -mcpu=arm926ej-s ts.s -o ts.o
arm-none-eabi-gcc -c -g -Wall -mcpu=arm926ej-s t.c -o t.o
arm-none-eabi-ld -T t.ld ts.o t.o -o t.elf
arm-none-eabi-objcopy -O binary t.elf t.bin

echo "ready to go?"
read dummy
#GDB="-S -s"
qemu-system-arm -M versatilepb -m 128M -kernel t.bin -serial mon:stdio ${GDB}
